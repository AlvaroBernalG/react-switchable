{
  "name": "react-switchable",
  "version": "0.0.1",
  "description": "A customisable react switch component",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "rm -rf dist/ && npm run build",
    "build:example": "babel src -d dist",
    "build": "webpack --config './webpack.config.dist.js'",
    "examples:dev": "webpack-dev-server --mode development",
    "format": "prettier --write '**/*.{js,jsx,json}'",
    "lint": "eslint --ext .js,.jsx src/ examples/src",
    "test": "jest",
    "test:watch": "jest --watch",
    "analyze-bundle":
      "webpack -p --profile --json > stats.json && webpack-bundle-analyzer stats.json examples/dist -s gzip"
  },
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0",
    "react-dom": "^15.3.0 || ^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.1.2",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "node-sass": "^4.9.0",
    "prettier": "^1.9.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.2"
  },
  "keywords": [
    "toggle",
    "custom",
    "toggle-button",
    "radiogroup",
    "checkbox",
    "react",
    "switch",
    "toggle-switch",
    "react-component",
    "switchable"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0"
  }
}
